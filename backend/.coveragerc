[run]
source = .
omit = 
    */old_app_directory/*
    */tests/*
    */__pycache__/*
    */.pytest_cache/*
    */venv/*
    */.venv/*
    */env/*
    setup.py
    */migrations/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = htmlcov